class Person {
    age = 0
    name = ""

    void constructor(string name) {
        self.age = 40
        self.name = name
    }

    void ageup() {
        self.age = self.age + 1
    }
}

fn deepest_factory() {
    return Person("John")
}

fn deeper_factory() {
    return deepest_factory()
}

fn test_factory() {
    return deeper_factory()
}

john := test_factory()

john.ageup()
john.ageup()
john.ageup()
john.ageup()

john.age